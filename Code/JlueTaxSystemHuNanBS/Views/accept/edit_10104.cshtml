                       }
                       else if(name=="wbsbzzsybnsrdkdjdkqd.kjnsrsbh"&&idx>4){
                           $("#dkdjsstyjksdkqd a.jiajian:first").click()
                       }

                   var td=$.sheet.FCT.getSheet(sbb.sbbname).getTd(name,idx)
                   if(td==null)
                   {
                       console.log("name:"+name+",index:"+idx)
                   }
                   else if(value!==undefined)
                   {
                       if(td.type=="list"){
                           td.list.val(value)
                           td.setValue(value)
                       }
                       else{
                           td.setValue(value)
                       }
                   }
               })
